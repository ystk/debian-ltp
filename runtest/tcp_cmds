arp arp01
echo export TCbin=$LTPROOT/testcases/bin; echo01 
finger finger01
ftp export TCbin=$LTPROOT/testcases/bin; ftp01
host host01
netstat netstat01
perf_lan export TCbin=$LTPROOT/testcases/bin; perf_lan
ping export LTPROOT; ping01
rcp export TCbin=$LTPROOT/testcases/bin; rcp01
rdist export TCbin=$LTPROOT/testcases/bin; rdist01
rlogin rlogin01
rsh rsh01
rwho rwho01
sendfile export TCbin=$LTPROOT/testcases/bin; sendfile01
tcpdump tcpdump01
telnet telnet01
iptables iptables_tests.sh
dhcpd dhcpd_tests.sh
